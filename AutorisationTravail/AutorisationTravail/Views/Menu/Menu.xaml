<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="AutorisationTravail.Views.Menu.Menu"
                  xmlns:MenuPages="clr-namespace:AutorisationTravail.Views.Menu"
                  xmlns:ListPages="clr-namespace:AutorisationTravail.Views.DemandeAutorisationTravail"
                  NavigationPage.HasNavigationBar="False">
    <MasterDetailPage.Master>
        <MenuPages:MenuMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <ListPages:ListDemandeAutorisationTravail />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>
